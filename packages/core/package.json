{
  "name": "@chaosmachine/core",
  "version": "1.0.3",
  "description": "Solidity smart contracts for the Chaos Machine",
  "keywords": [
    "ethereum",
    "solidity",
    "distributedgallery",
    "chaos",
    "machine",
    "jukebox"
  ],
  "author": "Distributed Gallery <contact@distributedgallery.com>",
  "contributors": [
    "Olivier Sarrouy <olivier@ryhope.network>"
